        data: {
            fname: 'cache_chr_fasta',
            gid: genome_id,
            chr: chr
        },
        success: function(data) {
        	var obj = jQuery.parseJSON(data);
        	export_to_irods("this chromosome's FASTA file", "export_fasta_chr", { chr: chr, file: obj.file });
