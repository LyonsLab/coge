function walk (direction) {
 //direction 1 == left -1 == right

 var num_seqs = $('#num_seqs').val();
 for (var i =1; i <= num_seqs; i++)
  {
    up = $('#drup'+i).val()*1;
    down = $('#drdown'+i).val()*1;
    dist = up + down;
    if (direction == 1)
     {
       $('#drup'+i).val(up+dist)
       $('#drdown'+i).val(down-dist)
     }
    else
     {
       $('#drup'+i).val(up-dist)
       $('#drdown'+i).val(down+dist)
     }
  }
}

