<TMPL_IF NAME='SHARE_DIALOG'>
<script>
$(function(){
	$("#share_input").autocomplete({
		source: [],
		focus: function() { return false; },
		select:
	    	function(event, ui) {
	    		$("#share_input")
	    			.val( ui.item.label )
	    			.data('select_id', ui.item.value);
	    		if (ui.item.value.split(':')[1] == ITEM_TYPE_USER) {
	    			$('#share_role_select').show();
	    		}
	    		else {
	    			$('#share_role_select').hide();
	    		}
	    		return false;
	    	},
	});
});
</script>
