Gobe.set_genespace= function (updown, info, value, length, anchorlength){
// console.log(updown, info, value, length, anchorlength);
/* e.g. ('up', 1, 330) becomes: byId('drup1').value = 330; */
/* so up is the 'left', down is the 'right' bar. */
    value = parseFloat(value);
    value = Math.min(value, updown == 'up' ? lefts[info.i] : rights[info.i]);
    var eid = 'dr' + updown + info.idx;
    $('#'+eid).val(value);
    value = value*-1;
    if (value < 1) {value = 1;}
    if (updown == "up")
     {
      var orig_gbstart = parseFloat($('#gbstart'+info.idx).val());
      var orig_dirstart = parseFloat($('#dirstart'+info.idx).val());
      $('#gbstart'+info.idx).val(value);
      $('#dirstart'+info.idx).val(value);
      // adjust length

       var gblength = $('#gblength'+info.idx).val();
       if (gblength == undefined || gblength == ""){gblength=length;}
       gblength = gblength-($('#gbstart'+info.idx).val()-orig_gbstart);
       $('#gblength'+info.idx).val(gblength);

       var dirlength = $('#dirlength'+info.idx).val();
       if (dirlength == undefined || dirlength == ""){dirlength=length;}
       dirlength = dirlength-($('#dirstart'+info.idx).val()-orig_dirstart);
       $('#dirlength'+info.idx).val(dirlength);
     }
    else if (updown == "down")
     {

       gblength = length-value-($('#gbstart'+info.idx).val()-pageObj.orig_gbstarts[info.idx]);
       $('#gblength'+info.idx).val(gblength);

       var tmp = (anchorlength-value+1)-parseFloat($('#dirstart'+info.idx).val());
       dirlength = length-value-($('#dirstart'+info.idx).val()-pageObj.orig_dirstarts[info.idx]);
       $('#dirlength'+info.idx).val(dirlength);
     }
};

