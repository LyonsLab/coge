function process_add_seq (newseq, num_seqs, go_run, refseq, old_num_seqs)
{
 $('#sorter').append(newseq);
 set_up_sort();
 if (!old_num_seqs)
  {
   old_num_seqs = num_seqs*1-1;
  }
 var i = old_num_seqs*1+1;
 while (i <= num_seqs)
