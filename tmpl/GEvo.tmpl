function new_left_slider(image_num, y, h) {
   var l = $('<canvas id="slider_up_' + image_num + '" style="position:absolute;left:0;top:' + y + 'px;width:11px;height:' + h + 'px" width="' + (11 * window.devicePixelRatio) + 'px" height="' + (h * window.devicePixelRatio) + 'px"></canvas>');
    var ctx = l[0].getContext('2d');
    ctx.scale(window.devicePixelRatio, window.devicePixelRatio);
    ctx.beginPath();
    ctx.moveTo(10.5, 0);
    ctx.quadraticCurveTo(-6, 8, 8.5, 16);
    ctx.lineTo(8.5, h - 16);
    ctx.quadraticCurveTo(-6, h - 8, 10.5, h);
    ctx.closePath();
    ctx.fillStyle = '#ccc';
    ctx.fill();
    ctx.strokeStyle = 'black';
    ctx.stroke();
    return l;

}

function new_right_slider(image_num, x, y, h) {
    var r = $('<canvas id="slider_down_' + image_num + '" style="position:absolute;left:' + x + 'px;top:' + y + 'px;width:11px;height:' + h + 'px" width="' + (11 * window.devicePixelRatio) + 'px" height="' + (h * window.devicePixelRatio) + 'px"></canvas>');
    var ctx = r[0].getContext('2d');
    ctx.scale(window.devicePixelRatio, window.devicePixelRatio);
    ctx.beginPath();
    ctx.moveTo(0.5, 0);
    ctx.quadraticCurveTo(16, 8, 2.5, 16);
    ctx.lineTo(2.5, h - 16);
    ctx.quadraticCurveTo(16, h - 8, 0.5, h);
    ctx.closePath();
    ctx.fillStyle = '#ccc';
    ctx.fill();
    ctx.strokeStyle = 'black';
    ctx.stroke();
    return r;
}

function drag_left(event, ui) {
    var id = ui.helper[0].id;
    var num = parseInt(id.substring(id.lastIndexOf('_') + 1));
    var info = image_info[basename + '_' + num + '.png'];
    var width = info.bpmax - info.bpmin + 1;
    var ratio = width / info.img_width;
    ui.position.left = Math.max(0, ui.position.left);
    ui.position.left = Math.min(ui.helper[0].r.position().left - 11, ui.position.left);
    Gobe.set_genespace('up', info, Math.round((info.xmin - ui.position.left - 0.5) * ratio), width, width * ratio);
}

function drag_right(event, ui) {
    var id = ui.helper[0].id;
    var num = parseInt(id.substring(id.lastIndexOf('_') + 1));
    var info = image_info[basename + '_' + num + '.png'];
    var width = info.bpmax - info.bpmin + 1;
    var ratio = width / info.img_width;
    ui.position.left = Math.max(ui.helper[0].l.position().left + 11, ui.position.left);
    ui.position.left = Math.min(info.img_width+11, ui.position.left);
    Gobe.set_genespace('down', info, Math.round((ui.position.left - 9.5 - info.xmax) * ratio), width, width * ratio);
}

function get_image_index(y) {
    for (var i=0; i<num_img; i++)
        if (y >= images[i].top && y <= images[i].top + image_height)
            return i;
    return -1;
}

function get_sequence_num(order) {
    for (var i=0; i<num_img; i++)
        if ($('#display_order' + images[i].index).val() == order)
            return i;
}

var images;
function setup_images() {
    images = [];
    var index = 1;
    while (images.length < num_img) {
        var img = $('#img' + index);
        if (img.length)
            images.push({img:img,index:index});
        ++index;
    }
}
function get_image(index) {
    for (var i=0; i<images.length; i++)
        if (images[i].index == index)
            return images[i];
}
var image_info;
var image_height;
var lefts;
var rights;
function images_loaded() {
    for (var i=0; i<num_img; i++) {
        if (!images[i].img[0].complete || images[i].img[0].naturalWidth === undefined || images[i].img[0].naturalWidth === 0) {
            setTimeout(images_loaded, 100);
            return;
        }
    }
    for (var i=0; i<num_img; i++) {
        images[i].top = images[i].img.offset().top - images[i].img.parent().offset().top;
        images[i].height = images[i].img.height();
    }
    $('#svg')[0].connectors = {};
    image_height = images[0].height;
    lefts = [];
    rights = [];
    var div = $('#images');
    for (var i=0; i<num_img; i++) {
        var top = images[i].top + 24;
        var h = images[i].height - 24;
        var l = new_left_slider(images[i].index, top, h).appendTo(div).draggable({axis:'x',drag:drag_left});
        var r = new_right_slider(images[i].index, images[i].img.width() + 11, top, h).appendTo(div).draggable({axis:'x',drag:drag_right});
        l[0].r = r;
        r[0].l = l;
        lefts.push(parseInt($('#drup' + images[i].index).val()));
        rights.push(parseInt($('#drdown' + images[i].index).val()));
    }
    $.ajax({
        dataType: 'json',
        url: 'gobe/gobe.py/' + basename + '/info/',
        success: function(data) {
            image_info = data;
        }
    });
}

function svg_click(event) {
    var bbox = false;
    drag_started = false;
    if (!document.getElementById('bbox').hidden) {
        document.getElementById('bbox').hidden = 1;
        bbox = true;
    }
    var x = event.offsetX;
    var y = event.offsetY;
    Gobe.handle_html('');
    var index = get_image_index(y);
    y -= images[index].top;
    var url = 'gobe/gobe.py/' + basename + '/query/?y=' + y + '&img=' + images[index].index + '&x=' + x;
    if (event.shiftKey || bbox)
        url += '&all=1';
    if (bbox)
        url += '&bbox=' + (Math.min(bbox_x1, bbox_x2) - 11) + ',' + (Math.min(bbox_y1, bbox_y2) - images[index].top) + ',' + (Math.max(bbox_x1, bbox_x2) - 11) + ',' + (Math.max(bbox_y1, bbox_y2) - images[index].top);
    $.ajax({
        dataType: 'json',
        url: url,
        success: function(data) {
            data = data.resultset;
            var wedges = $('#set_wedges').css('display') == 'none';
            var line_width = $('#line_width_val').val();
            var svg = document.getElementById('svg');
            for (var i=0; i<data.length; i++) {
                var pair = data[i];
                Gobe.handle_html('<p><a target="_blank" href="' + pair.link + '">full annotation</a></p>&#10;&#10;<p>' + pair.annotation + '</p>');
                if (!pair.has_pair)
                    continue;
                var color = '#' + pair.color.substring(2);
                var keys = [];
                for (key in pair.features)
                    keys.push(key);
                keys.sort();
                var coords1 = pair.features[keys[0]];
                var coords2 = pair.features[keys[1]];
                if (svg.connectors[coords1[4]])
                    continue;
                svg.connectors[coords1[4]] = true;
                var y1 = get_image(keys[0].substring(3)).top;
                var y2 = get_image(keys[1].substring(3)).top;
                var r1x1 = coords1[0] + 11;
                var r1x2 = coords1[2] + 11;
                var r2x1 = coords2[0] + 11;
                var r2x2 = coords2[2] + 11;
                var r1y = coords1[3] + y1;
                var r2y = coords2[1] + y2;
                var r1 = newSVG('rect', {x: r1x1, y: coords1[1] + y1, width: r1x2 - r1x1, height: coords1[3] - coords1[1], style: 'fill-opacity:0;stroke:black;stroke-width:0.5;', onclick: 'remove_connector(this);event.stopPropagation();'});
                var r2 = newSVG('rect', {x: r2x1, y: r2y, width: r2x2 - r2x1, height: coords2[3] - coords2[1], style: 'fill-opacity:0;stroke:black;stroke-width:0.5;', onclick: 'remove_connector(this);event.stopPropagation();'});
                var c;
                if (wedges)
                    c = newPolygon(r1x1 + ',' + r1y + ' ' + r2x1 + ',' + r2y + ' ' + r2x2 + ',' + r2y + ' ' + r1x2 + ',' + r1y, color);
                else
                    c = newLine((r1x1 + r1x2) / 2, r1y, (r2x1 + r2x2) / 2, r2y, color, line_width);
                r1.r2 = r2;
                r1.c = c;
                r2.r1 = r1;
                r2.c = c;
                c.r1 = r1;
                c.r2 = r2;
                c.connector_id = coords1[4];
                svg.appendChild(r1);
                svg.appendChild(r2);
                svg.appendChild(c);
            }
        }
    });
}

var bbox_x1 = 0;
var bbox_y1 = 0;
var bbox_x2 = 0;
var bbox_y2 = 0;
var bbox_img;
var drag_started = false;
function calc_bbox() {
    var x1 = Math.min(bbox_x1, bbox_x2);
    x1 = Math.max(x1, 11);
    var x2 = Math.max(bbox_x1, bbox_x2);
    x2 = Math.min(x2, 1011);
    var y1 = Math.min(bbox_y1, bbox_y2);
    y1 = Math.max(y1, images[bbox_img].top);
    var y2 = Math.max(bbox_y1, bbox_y2);
    y2 = Math.min(y2, images[bbox_img].top + image_height);
    var div = document.getElementById('bbox');
    div.style.left = x1 + 'px';
    div.style.top = y1 + 'px';
    div.style.width = x2 - x1 + 'px';
    div.style.height = y2 - y1 + 'px';
}
function svg_mousedown(e) {
    bbox_img = get_image_index(e.offsetY);
    if (bbox_img != -1) {
        bbox_x1 = e.offsetX;
        bbox_y1 = e.offsetY;
        drag_started = true;
    }
};
function svg_mousemove(e) {
    if (drag_started) {
        if (Math.abs(e.offsetX - bbox_x1) > 2) {
            drag_started = false;
            bbox_x2 = e.offsetX;
            bbox_y2 = e.offsetY;
            calc_bbox();
            document.getElementById('bbox').hidden = 0;
            return;
        }
    }
    if (!document.getElementById('bbox').hidden) {
        bbox_x2 = e.offsetX;
        bbox_y2 = e.offsetY;
        calc_bbox();
    }
};
function svg_mouseleave(e) {
    drag_started = false;
    document.getElementById('bbox').hidden = 1;
}

function remove_connector(e) {
    drag_started = false;
    if (!document.getElementById('bbox').hidden) {
        document.getElementById('bbox').hidden = 1;
        return;
    }
    if (e.connector_id)
        delete $('#svg')[0].connectors[e.connector_id];
    var p = e.parentNode;
    if (e.r1)
        p.removeChild(e.r1);
    if (e.r2)
        p.removeChild(e.r2);
    if (e.c) {
        delete $('#svg')[0].connectors[e.c.connector_id];
        p.removeChild(e.c);
    }
    p.removeChild(e);
}

function set_connector(type) {
    $('#svg').find(type == 'line' ? 'polygon' : 'line').each(function(i, e) {
        var c;
        var r1x1 = parseFloat(e.r1.getAttribute('x'));
        var r1x2 = r1x1 + parseFloat(e.r1.getAttribute('width'));
        var r1y = parseFloat(e.r1.getAttribute('y')) + parseFloat(e.r1.getAttribute('height'));
        var r2x1 = parseFloat(e.r2.getAttribute('x'));
        var r2x2 = r2x1 + parseFloat(e.r2.getAttribute('width'));
        var r2y = parseFloat(e.r2.getAttribute('y'));
        if (type == 'line') {
            var line_width = $('#line_width_val').val();
            c = newLine((r1x1 + r1x2) / 2, r1y, (r2x1 + r2x2) / 2, r2y, e.style.stroke, line_width);
        } else
            c = newPolygon(r1x1 + ',' + r1y + ' ' + r2x1 + ',' + r2y + ' ' + r2x2 + ',' + r2y + ' ' + r1x2 + ',' + r1y, e.style.stroke);
        c.r1 = e.r1;
        c.r1.c = c;
        c.r2 = e.r2;
        c.r2.c = c;
        c.connector_id = e.connector_id;
        e.parentNode.replaceChild(c, e);
    });
}

function newLine(x1,y1,x2,y2,color,width) {
    return newSVG('line', {x1: x1, y1: y1, x2: x2, y2: y2, style: 'stroke:' + color + ';stroke-width:' + width + ';stroke-linecap:round;stroke-opacity:.6;', onclick: 'remove_connector(this);event.stopPropagation();'});
}

function newPolygon(points, color) {
    return newSVG('polygon', {points: points, style: 'fill:' + color + ';fill-opacity:0.3;stroke:' + color + ';stroke-width:0.5;', onclick: 'remove_connector(this);event.stopPropagation();'});
}

function newSVG(tag, attrs) {
    var el= document.createElementNS('http://www.w3.org/2000/svg', tag);
    for (var k in attrs)
        el.setAttribute(k, attrs[k]);
    return el;
}
